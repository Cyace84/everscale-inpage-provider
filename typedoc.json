{
  "entryPoints": ["src/index.ts"],
  "out": "docs/.build",
  "readme": "none",
  "excludePrivate": true,
  "includeVersion": true,
  "categorizeByGroup": false,
  "categoryOrder": ["Provider", "Contract", "Stream", "Models", "Provider Api", "Utils", "*"],
  "sort": ["source-order"],
  "hideGenerator": true,
  "customCss": "typedoc.css",
  "plugin": ["typedoc-plugin-markdown"],
  "excludeExternals": true,
  "markedOptions": {
    "codeBlock": "fenced"
  }
}
